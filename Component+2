    "## This code comes from Prof Turk. Translates our 4 hour time period into the time of day between 2:46pm and 6:46pm. \n",
    "v = pd.date_range(\"2:46PM\", \"6:46PM\", freq=\"1s\")\n",
    "v -= v[0]\n",
    "array_vals[\"time\"] = v\n",
    "array_vals.set_index(\"time\", inplace=True)"
